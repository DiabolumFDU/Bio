  ipgui::add_param $IPINST -name "DATA_WIDTH" -parent ${Page_0}
  ipgui::add_param $IPINST -name "FORM_WIDTH" -parent ${Page_0}
  ipgui::add_param $IPINST -name "RESULT_WID" -parent ${Page_0}


}

